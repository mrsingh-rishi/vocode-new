{
  "to_phone": "+1234567890",
  "env_vars": {
    "twilio_account_sid": "ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx",
    "twilio_auth_token": "your_twilio_auth_token_here",
    "twilio_phone_number": "+1987654321",
    "openai_api_key": "sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx",
    "deepgram_api_key": "your_deepgram_api_key_here",
    "eleven_labs_api_key": "your_elevenlabs_api_key_here",
    "eleven_labs_voice_id": "9BWtsMINqrJLrRacOk9x"
  },
  "agent": {
    "type": "chatgpt",
    "config": {
      "model_name": "gpt-4o",
      "temperature": 0.7,
      "max_tokens": 150,
      "frequency_penalty": 0.0,
      "presence_penalty": 0.0
    }
  },
  "transcriber": {
    "type": "deepgram",
    "config": {
      "model": "nova-2",
      "language": "en-US",
      "smart_format": true,
      "punctuate": true,
      "diarize": false,
      "multichannel": false,
      "alternatives": 1,
      "profanity_filter": true,
      "redact": [],
      "keywords": []
    }
  },
  "synthesizer": {
    "type": "eleven_labs",
    "config": {
      "model_id": "eleven_flash_v2_5",
      "stability": 0.75,
      "similarity_boost": 0.75,
      "style": 0.0,
      "use_speaker_boost": true,
      "optimize_streaming_latency": 1,
      "experimental_streaming": false,
      "experimental_websocket": false
    }
  },
  "telephony": {
    "type": "twilio",
    "config": {
      "record": false,
      "recording_status_callback": null,
      "machine_detection": "none"
    }
  },
  "prompt_preamble": "You are a professional AI assistant making a phone call. Be friendly, helpful, and speak naturally. Keep your responses concise and conversational.",
  "initial_message": "Hi there! This is an AI assistant calling to help you today. How are you doing?"
}
