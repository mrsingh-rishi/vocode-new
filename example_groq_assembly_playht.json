{
  "to_phone": "+1234567890",
  "env_vars": {
    "twilio_account_sid": "your-twilio-account-sid",
    "twilio_auth_token": "your-twilio-auth-token",
    "twilio_phone_number": "+1987654321",
    "groq_api_key": "your-groq-api-key",
    "assembly_ai_api_key": "your-assembly-ai-api-key",
    "play_ht_api_key": "your-playht-api-key",
    "play_ht_user_id": "your-playht-user-id"
  },
  "agent": {
    "type": "groq",
    "config": {
      "model_name": "llama3-70b-8192",
      "temperature": 0.9
    }
  },
  "transcriber": {
    "type": "assembly_ai",
    "config": {
      "punctuate": true,
      "format_text": true
    }
  },
  "synthesizer": {
    "type": "play_ht",
    "config": {
      "voice": "larry",
      "speed": 1.0
    }
  },
  "prompt_preamble": "You are powered by Groq's fast LLM inference. Be energetic and quick in your responses.",
  "initial_message": "Hey there! This is a super-fast AI calling you. What's up?"
}
